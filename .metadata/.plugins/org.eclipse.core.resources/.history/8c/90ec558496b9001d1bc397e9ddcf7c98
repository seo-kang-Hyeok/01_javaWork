package test02;
class Person {
	private String name;
	private int age;
	
	Person() {}
	
	Person(String name, int age) {
		this.name = name;
		this.age = age;
	}
	@Override
	public String toString() {
		return "이름 : " + name + ", 나이 : " + age;
	}
}

public class prersonTest {
	public static void main(String[] args) {
		Person p1 = new Person("강혁",7);
		
	}
}
