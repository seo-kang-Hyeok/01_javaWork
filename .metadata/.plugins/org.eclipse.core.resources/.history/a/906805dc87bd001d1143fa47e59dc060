package collection05.vector01;

import java.util.Vector;

public class vector2 {

	public static void main(String[] args) {
		//매개변수로 어떠한 객체 다 받겠다. 권장하지 않
		Vector v = new Vector();
		
		v.add(1);
		v.add(8.89);
		v.add("478");
		v.add(new Integer(8));
		v.add(new Double(6.89));
		v.add(new String("kh정보교육원"));
		
	}
}
