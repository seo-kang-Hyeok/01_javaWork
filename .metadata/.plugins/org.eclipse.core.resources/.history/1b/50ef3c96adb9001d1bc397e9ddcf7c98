package chap_04;

public class _05_For {

	public static void main(String[] args) {
		//반복문 For
		//나코 매장
		System.out.println("어서오세요, 나코입니다.");
		//또다른 손님이 들어오면?
		System.out.println("어서오세요, 나코입니다.");
		System.out.println("어서오세요, 나코입니다.");
		System.out.println("어서오세요, 나코입니다.");
		System.out.println("어서오세요, 나코입니다.");
		System.out.println("어서오세요, 나코입니다.");
		
	}
}
