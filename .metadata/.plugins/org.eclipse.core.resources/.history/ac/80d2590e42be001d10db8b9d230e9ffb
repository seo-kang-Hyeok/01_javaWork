package day17과제;

import java.util.Scanner;

public class 약수 {
//	 int num1;
//	 int num2;
//	public 약수(int a,int b) {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println(" 숫자를 입력하세요. : ");
		int a = sc.nextInt();

		
		for (int i = 1; i <= a; i++) {
				if (a % i == 0) {
					System.out.print(i + ",");
					}
		}

	}
}
