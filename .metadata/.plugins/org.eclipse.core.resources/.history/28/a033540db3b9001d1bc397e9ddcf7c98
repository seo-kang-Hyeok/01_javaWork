package chap_04;

public class _08_nestedLoop {

	public static void main(String[] args) {
		//이중 반복문
		
		
		//별 (*) 사각형 만들기
//		*****
//		*****
//		*****
//		*****
//		*****

		for (int i = 0; i < 5; i++) {
			System.out.print("*");
			System.out.print("*");
			System.out.print("*");
			System.out.print("*");
			System.out.print("*");
			System.out.print("*");
			for (int j = 0; j < 5; j++) {
				
				
				
		}
		System.out.println();
		}
	
	}
}
