package day17과제;

import java.util.Scanner;

public class Gcd {
	
		int a ;
		int b ;

		public void gcd(){
			
			int min = (num1 < num2) ? num1 : num2;   
			int gcd = 0;
				for (int i = 1; i <= min; i++) {
					if (num1 % i == 0 && num2 % i == 0)
						gcd = i;
			}
	System.out.println(a+" 와 "+ b + " 최대공약수 : " + gcd);

}
}
